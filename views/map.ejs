<!DOCTYPE html>
<html>
  <head>
    <title>Custom Markers</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <style>
      #map {
        height: 100%;
      }
      html, body {
        height: 100%;
        margin: 0;
        padding: 0;
      }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script>
    var map;
      function initMap() {
        map = new google.maps.Map(
            document.getElementById('map'),
            {center: new google.maps.LatLng(17.4378471,78.7161455), zoom: 16});
        var iconBase ='./assets/icons/';
      var r= "<%= p%>";
      var d = JSON.parse(r);
      for(var i=0;i<d.length;i++)
       {
         var marker = new google.maps.Marker({
            position: new google.maps.LatLng(d[i].lat,d[i].long),
            icon: iconBase+d[i].cr+".png",
            map: map
          });
       }
    }
    </script>
    </body>
</html>
